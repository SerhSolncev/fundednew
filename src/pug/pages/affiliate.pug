                        var withList = [
                            {
                                icon: 'content/images/icons/with1.svg',
                                title: 'Account Access',
                                text: 'Access a wide range of markets from Stocks, Metals, Indices to FX. We have something for everyone.'
                            }, {
                                icon: 'content/images/icons/with2.svg',
                                title: 'Fastest Payout',
                                text: 'Access a wide range of markets from Stocks, Metals, Indices to FX. We have something for everyone.'
                            }, {
                                icon: 'content/images/icons/with3.svg',
                                title: 'Pro Support',
                                text: 'Access a wide range of markets from Stocks, Metals, Indices to FX. We have something for everyone.'
                            }, {
                                icon: 'content/images/icons/with4.svg',
                                title: 'Reset Topup',
                                text: 'Access a wide range of markets from Stocks, Metals, Indices to FX. We have something for everyone.'
                            }, {
                                icon: 'content/images/icons/with5.svg',
                                title: 'Best Trading Condition',
                                text: 'Access a wide range of markets from Stocks, Metals, Indices to FX. We have something for everyone.'
